{
  "id": "baidu",
  "name": "百度",
  "baseUrl": "https://www.baidu.com",
  "searchPath": "/s?wd=",
  "selectors": {
    "resultContainer": "div.result, .c-container",
    "title": "h3.t a, h3 a",
    "link": "h3.t a, h3 a",
    "snippet": "[data-module=\"abstract\"], [data-module=\"sc_p\"], .c-abstract"
  },
  "headers": {
  },
  "antiBot": {
    "enabled": true,
    "detectors": [],
    "errorMessage": "百度需要人工验证，请手动完成后重试。"
  },
  "customDelay": {
    "min": 1500,
    "max": 4000
  },
  "fallbackSelector": "div:has(a[href*=\"http\"])",
  "linkValidation": [
    "http",
    "www"
  ]
}
